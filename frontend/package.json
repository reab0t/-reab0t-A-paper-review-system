    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.0",
    "@vue/tsconfig": "^0.4.0",
    "cypress": "^13.5.0",
    "eslint": "^8.53.0",
    "eslint-plugin-vue": "^9.18.0",
    "prettier": "^3.0.0",
    "sass": "^1.69.0",
    "typescript": "^5.2.0",
